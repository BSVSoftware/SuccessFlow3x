        <SQL_FORM DB="SQL" RESTAB="SQL">
          <SQL_STMT_U val="Update dbo.STAT_TonerZaehlerChange set PreviousAfterChangeDate = (&#xD;&#xA;&#x9;Select case when&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select  c.AfterChangeDate  from STAT_TonerZaehlerChange c where c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and c.IDNR=a.IDNR and a.Coleranttype=c.Coleranttype and c.AfterChangeDate=&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select MAX(b.AfterChangeDate) from STAT_TonerZaehlerChange b where b.AfterChangeDate&lt;a.AfterChangeDate and a.Coleranttype=b.Coleranttype and a.IDNR=b.IDNR and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) is null &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE WHEN a.Coleranttype='Cyan' THEN (Select min(b.Datum) from STAT_TonerZaehler b where a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Cyan is not null ) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN a.Coleranttype='Magenta' THEN (Select min(b.Datum) from STAT_TonerZaehler b where a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Magenta is not null) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN a.Coleranttype='Yellow' THEN (Select min(b.Datum) from STAT_TonerZaehler b where a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Yellow is not null) &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN a.Coleranttype='Black' THEN (Select min(b.Datum) from STAT_TonerZaehler b where a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Black is not null) &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select  c.AfterChangeDate  from STAT_TonerZaehlerChange c where c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and c.IDNR=a.IDNR and a.Coleranttype=c.Coleranttype and c.AfterChangeDate=&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select MAX(b.AfterChangeDate) from STAT_TonerZaehlerChange b where b.AfterChangeDate&lt;a.AfterChangeDate and a.Coleranttype=b.Coleranttype and a.IDNR = b.IDNR and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)  end)&#xD;&#xA;FROM dbo.STAT_TonerZaehlerChange a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_TonerZaehlerChange set PreviousAfterChangeValue = (&#xD;&#xA;&#x9;Select case when&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select  c.AfterChangeValue  from STAT_TonerZaehlerChange c where c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and c.IDNR=a.IDNR and a.Coleranttype=c.Coleranttype and c.AfterChangeDate=&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select MAX(b.AfterChangeDate) from STAT_TonerZaehlerChange b where b.AfterChangeDate&lt;a.AfterChangeDate and a.Coleranttype=b.Coleranttype and a.IDNR=b.IDNR and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;) is null &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;then &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CASE WHEN a.Coleranttype='Cyan' THEN (Select b.cyan from STAT_TonerZaehler b where a.PreviousAfterChangeDate=b.Datum and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Cyan is not null ) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN a.Coleranttype='Magenta' THEN (Select b.Magenta from STAT_TonerZaehler b where a.PreviousAfterChangeDate=b.Datum and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Magenta is not null) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN a.Coleranttype='Yellow' THEN (Select b.Yellow from STAT_TonerZaehler b where a.PreviousAfterChangeDate=b.Datum and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Yellow is not null) &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHEN a.Coleranttype='Black' THEN (Select b.Black from STAT_TonerZaehler b where a.PreviousAfterChangeDate=b.Datum and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDNR=a.IDNR and b.Black is not null) &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select  c.AfterChangeValue  from STAT_TonerZaehlerChange c where c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and c.IDNR=a.IDNR and a.Coleranttype=c.Coleranttype and c.AfterChangeDate=&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Select MAX(b.AfterChangeDate) from STAT_TonerZaehlerChange b where b.AfterChangeDate&lt;a.AfterChangeDate and a.Coleranttype=b.Coleranttype and a.IDNR = b.IDNR and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)  end)&#xD;&#xA;FROM dbo.STAT_TonerZaehlerChange a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;&#xD;&#xA;--*** Datumswerte fÃ¼r ACDate-1 in STAT_TonerZaehler generieren falls nicht vorhanden!&#xD;&#xA;Insert into STAT_TonerZaehler (Ebene1,Ebene2,Ebene3,Ebene4, KundNr,ModellNr,IDNR,DeviceID,datum)&#xD;&#xA;Select a.Ebene1,a.Ebene2,a.Ebene3,a.Ebene4, a.KundNr, a.ModellNr, a.IDNR,a.DeviceID,a.AfterChangeDate-1 from STAT_TonerZaehlerChange a where (Select b.datum from STAT_TonerZaehler b where  b.Ebene1=a.Ebene1 and b.Ebene2=a.Ebene2 and b.Ebene3=a.Ebene3 and b.Ebene4=a.Ebene4 and a.IDNR=b.IDNR and b.datum=a.AfterchangeDate-1) is null and NoTonerChange_Flag=0 and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4"/>
