          <SQL_FORM DB="SQL" RESTAB="SQL">
            <SQL_STMT_U val="insert into STAT_LagerStrukturAnalyseNL (Ebene1,Ebene2,Ebene3,Ebene4,ARTNR,LagerNr,Bestand) &#xD;&#xA;select st.Ebene1,st.Ebene2,st.Ebene3,st.Ebene4,st.ARTNR,nl.LagerNr,isnull(nl.Bestand,0) from STAT_LagerStrukturAnalyse st join&#xD;&#xA; NEBENLAGER nl on (nl.Ebene1=st.Ebene1 and nl.Ebene2=st.Ebene2 and nl.Ebene3=st.Ebene3 and nl.Ebene4=st.Ebene4 and nl.ArtikelNr=st.ARTNR)&#xD;&#xA;where st.Ebene1=:1 and st.Ebene2=:2 and st.Ebene3=:3 and st.Ebene4=:4 and nl.LagerNr>1&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_LagerStrukturAnalyseNL set Jahresverbrauch = isnull((Select sum(Case when isnull(b.Teilung,0)&lt;>0 then b.Liefermenge/b.Teilung else 0 end) FROM POSITIONEN b left outer join AUFTRAGKOPF ak on b.Ebene1=ak.Ebene1 and b.Ebene2=ak.Ebene2 and b.Ebene3=ak.Ebene3 and b.Ebene4=ak.Ebene4 and b.Auftnr=ak.AuftragsNr where (ak.an_Lager_buchen is null or ak.an_Lager_buchen=0) and (ak.Geschaeftsfall is null or ak.Geschaeftsfall&lt;>'') and (dateadd(MM,datediff(MM,0,(dateadd(MM, datediff(MM,0,b.Auftrags_Datum),0))),0))>= (dateadd(MM,datediff(MM,0,(dateadd(MM, datediff(MM,0,':5')-12,0))),0)) and (dateadd(MM,datediff(MM,0,(dateadd(MM, datediff(MM,0,b.Auftrags_Datum),0))),0))&lt;= (dateadd(MM,datediff(MM,0,(dateadd(MM, datediff(MM,0,':5'),-1))),0)) and a.ARTNR=b.Artikel_Nr and a.LagerNr=b.Lagernummer and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 ),0) from dbo.STAT_LagerStrukturAnalyseNL a&#xD;&#xA;"/>
