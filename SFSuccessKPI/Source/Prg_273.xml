        <SQL_FORM DB="SQL" RESTAB="SQL">
          <SQL_STMT_U val="Delete from dbo.STAT_Trend_Prognose_Anbahnung where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;DECLARE @date datetime,@IDNR integer;&#xD;&#xA;&#x9;&#x9;SET @date = (dateadd(month,datediff(MONTH,0,(dateadd(yy, datediff(yy,0,GETDATE())-2,0))),0))&#xD;&#xA;&#x9;&#x9;SET @IDNR=1&#xD;&#xA;&#x9;&#x9;WHILE @date&lt;= dateadd(MM,12,getdate())&#xD;&#xA;&#x9;&#x9;&#x9;BEGIN&#xD;&#xA;&#x9;&#x9;&#x9;insert into dbo.STAT_Trend_Prognose_Anbahnung(MonthStartDate,Ebene1,Ebene2,Ebene3,Ebene4,IDnr)&#xD;&#xA;&#x9;&#x9;&#x9;Select&#x9;@date, :1,:2,:3,:4,@IDNR&#xD;&#xA;&#x9;&#x9;&#x9;SET @date = dateadd(MM,1,@date)&#xD;&#xA;&#x9;&#x9;&#x9;SET @IDNR=@IDNR+1&#xD;&#xA;&#x9;&#x9;&#x9;END&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_Trend_Prognose_Anbahnung set MaxDatum = (Select max(Datum)FROM STAT_Anbahnung_KPI_daily b where a.MonthStartDate = dateadd(month, datediff(month, 0, b.Datum),0) and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by dateadd(month, datediff(month, 0, b.Datum),0))&#xD;&#xA; from dbo.STAT_Trend_Prognose_Anbahnung a where a.MonthStartDate&lt;=dateadd(month, datediff(month, 0, getdate()),0) and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;Update dbo.STAT_Trend_Prognose_Anbahnung set UmsatzPlan = isnull((Select sum(b.UmsatzPlan) FROM Z_ANBAHNUNG b where a.MonthStartDate = dateadd(month, datediff(month, 0, b.erstelltam),0)and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by dateadd(month, datediff(month, 0, b.erstelltam),0)),0) from dbo.STAT_Trend_Prognose_Anbahnung a where a.MonthStartDate&lt;=dateadd(month, datediff(month, 0, getdate()),0) and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;Update dbo.STAT_Trend_Prognose_Anbahnung set UmsatzPlanReal = isnull((Select sum(b.UmsatzPlan*b.Wahrscheinlichkeit/100) FROM Z_ANBAHNUNG b where a.MonthStartDate = dateadd(month, datediff(month, 0, b.erstelltam),0)and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by dateadd(month, datediff(month, 0, b.erstelltam),0)),0) from dbo.STAT_Trend_Prognose_Anbahnung a where a.MonthStartDate&lt;=dateadd(month, datediff(month, 0, getdate()),0) and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;Update dbo.STAT_Trend_Prognose_Anbahnung set AnbAnzahl = isnull((Select count(distinct b.AnbahnungsNr)FROM Z_ANBAHNUNG b where a.MonthStartDate = dateadd(month, datediff(month, 0, b.erstelltam),0)and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by dateadd(month, datediff(month, 0, erstelltam),0)),0) from dbo.STAT_Trend_Prognose_Anbahnung a where a.MonthStartDate&lt;=dateadd(month, datediff(month, 0, getdate()),0) and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;--Durchschnittswerte bei denen der Nenner=0 werden gelÃ¶scht&#xD;&#xA;update dbo.STAT_Trend_Prognose_Anbahnung SET UmsatzPlanRealAnb = case when AnbAnzahl&lt;>0 then UmsatzPlanReal/AnbAnzahl else 0 end where MonthStartDate&lt;=dateadd(month, datediff(month, 0, getdate()),0) and Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;update dbo.STAT_Trend_Prognose_Anbahnung SET UmsatzPlanRealAnb = Null where AnbAnzahl=0 and Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;update dbo.STAT_Trend_Prognose_Anbahnung SET UmsatzPlanAnb = case when AnbAnzahl&lt;>0 then UmsatzPlan/AnbAnzahl else 0 end where MonthStartDate&lt;=dateadd(month, datediff(month, 0, getdate()),0) and Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;update dbo.STAT_Trend_Prognose_Anbahnung SET UmsatzPlanAnb = NULL where  AnbAnzahl=0 and Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4"/>
