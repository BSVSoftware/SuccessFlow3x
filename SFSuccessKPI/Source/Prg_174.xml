        <SQL_FORM DB="SQL" RESTAB="SQL">
          <SQL_STMT_U val="Delete from dbo.STAT_Dashboard_QuarterDay where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;DECLARE @date datetime;&#xD;&#xA;&#x9;&#x9;SET @date = (dateadd(month,datediff(MONTH,0,(dateadd(quarter, datediff(quarter,0,getdate()),0))),0))&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;WHILE @date&lt;= (dateadd(QUARTER,datediff(Quarter,0,getdate())+1,0))-1&#xD;&#xA;&#x9;&#x9;&#x9;BEGIN&#xD;&#xA;&#x9;&#x9;&#x9;insert into dbo.STAT_Dashboard_QuarterDay (QuarterDay,Ebene1,Ebene2,Ebene3,Ebene4)&#xD;&#xA;&#x9;&#x9;&#x9;Select&#x9;@date, :1, :2, :3, :4&#xD;&#xA;&#x9;&#x9;&#x9;SET @date = dateadd(DD,1,@date)&#xD;&#xA;&#x9;&#x9;&#x9;END&#x9;&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_Dashboard_QuarterDay set NettoCY = (Select sum(b.Netto)FROM RECHNUNGSBUCH b where a.QuarterDay = b.Datum and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by Datum) from dbo.STAT_Dashboard_QuarterDay a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;Update dbo.STAT_Dashboard_QuarterDay set NettoLY = (Select sum(b.Netto)FROM RECHNUNGSBUCH b where a.QuarterDay = dateadd(Year,1,b.Datum) and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by Datum) from dbo.STAT_Dashboard_QuarterDay a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;Update dbo.STAT_Dashboard_QuarterDay set DeckungCY = (Select sum(b.Deckung)FROM POSITIONEN b where a.QuarterDay = b.Rechnungsdatum and a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 group by Rechnungsdatum) from dbo.STAT_Dashboard_QuarterDay a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;&#xD;&#xA;--- a.MonthDay&lt;=GETDATE() stellt sicher daÃŸ Rtot nur bis zum CurrentDate dargestellt wird!&#xD;&#xA;Update dbo.STAT_Dashboard_QuarterDay set RtotNettoCY = &#xD;&#xA;&#x9;&#x9;(&#xD;&#xA;&#x9;&#x9;SELECT  sum(b.NettoCY)&#xD;&#xA;&#x9;&#x9;FROM  dbo.STAT_Dashboard_QuarterDay b&#xD;&#xA;&#x9;&#x9;where (a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDnr &lt;= a.IDnr and a.QuarterDay&lt;=GETDATE())&#xD;&#xA;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;from dbo.STAT_Dashboard_QuarterDay a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_Dashboard_QuarterDay set RtotNettoLY = &#xD;&#xA;&#x9;&#x9;(&#xD;&#xA;&#x9;&#x9;SELECT  sum(b.NettoLY)&#xD;&#xA;&#x9;&#x9;FROM  dbo.STAT_Dashboard_QuarterDay b&#xD;&#xA;&#x9;&#x9;where (a.Ebene1=b.Ebene1 and a.Ebene2=b.Ebene2 and a.Ebene3=b.Ebene3 and a.Ebene4=b.Ebene4 and b.IDnr &lt;= a.IDnr)&#xD;&#xA;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;from dbo.STAT_Dashboard_QuarterDay a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;"/>
