        <SQL_FORM DB="SQL" RESTAB="SQL">
          <SQL_STMT_U val="insert into dbo.STAT_Hotline_Daten (Ebene1,Ebene2,Ebene3,Ebene4,Jobnr,KundNr,Username,datumtyp,Datum, derstat, Anfangsdatum,Jobstatus,AbschlussdatumSoll)&#xD;&#xA;select ebene1,ebene2,ebene3,ebene4,lfd_Job_Nr,KundenNr,Username,-1, Abschlussdatum, derzeitiger_Status,Rufeingang_Datum, Job_beendet,Abschlussdatum from HOTLINE&#xD;&#xA;where  Ebene1=:1 and  Ebene2=:2 and  Ebene3=:3 and Ebene4=:4 and Job_beendet=1 and Rufeingang_Datum >=':11'  and IDNR>0 and derzeitiger_Status not in (':5', ':6', ':7', ':8', ':9', ':10') and Abschlussdatum is not null&#xD;&#xA;union all&#xD;&#xA;select ebene1,ebene2,ebene3,ebene4,lfd_Job_Nr,KundenNr,Username,1, rufeingang_datum, derzeitiger_Status, rufeingang_datum, Job_beendet,Abschlussdatum from hotline&#xD;&#xA;where  Ebene1=:1 and  Ebene2=:2 and  Ebene3=:3 and Ebene4=:4 and lfd_Job_Nr>0 and Rufeingang_Datum >=':11'  and IDNR>0  and derzeitiger_Status not in  (':5', ':6', ':7', ':8', ':9', ':10')&#xD;&#xA;group by ebene1,ebene2,ebene3,ebene4, Rufeingang_Datum, lfd_Job_Nr,KundenNr,Username,derzeitiger_Status,Job_beendet,Abschlussdatum&#xD;&#xA;order by 9&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;update dbo.STAT_Hotline_Daten set Matchcode =&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT  &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT b.Matchcode from KUNDEN b where  c.KundNr=b.KDNR and c.Ebene1=b.Ebene1 and c.Ebene2=b.Ebene2 and c.Ebene3=b.Ebene3 and c.Ebene4=b.Ebene4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM  dbo.STAT_Hotline_Daten c&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where (c.IDnr = a.IDnr )&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)FROM dbo.STAT_Hotline_Daten as a&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;update dbo.STAT_Hotline_Daten set BEENDETDauer = 1 +&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;(SELECT  datediff(dd, b.Anfangsdatum, a.Datum)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;FROM  dbo.STAT_Hotline_Daten b&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;where (b.IDnr = a.IDnr  and datumtyp=-1)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;from dbo.STAT_Hotline_Daten as a&#xD;&#xA;&#xD;&#xA;update dbo.STAT_Hotline_Daten set OFFENDauer = 1 +&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;(SELECT  datediff(dd, b.Datum, getdate())&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;FROM  dbo.STAT_Hotline_Daten b&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;where (b.IDnr = a.IDnr  and datumtyp=1 and JobStatus=0)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;from dbo.STAT_Hotline_Daten as a&#xD;&#xA;"/>
