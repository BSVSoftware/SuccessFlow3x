        <SQL_FORM DB="SQL" RESTAB="SQL">
          <SQL_STMT_U val="update STAT_TonerZaehlerChange set d_VolumenToner=(Select  CASE WHEN a.coleranttype='Black'&#x9;THEN&#x9;(Select c.Zaehler_Gesamt from STAT_TonerZaehler c where c.Datum=a.ChangeDate_extrapol and c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and a.IDNR =c.IDNR)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;- &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select c.Zaehler_Gesamt from STAT_TonerZaehler c where c.Datum=a.ChangeDate_Previous and c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and a.IDNR =c.IDNR)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select c.Zaehler_Farbe from STAT_TonerZaehler c where c.Datum=a.ChangeDate_extrapol and c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and a.IDNR =c.IDNR)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;- &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select c.Zaehler_Farbe from STAT_TonerZaehler c where c.Datum=a.ChangeDate_Previous and c.Ebene1=a.Ebene1 and c.Ebene2=a.Ebene2 and c.Ebene3=a.Ebene3 and c.Ebene4=a.Ebene4 and a.IDNR =c.IDNR)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END) from STAT_TonerZaehlerChange a where a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xD;&#xA;&#xD;&#xA;update dbo.STAT_TonerZaehlerChange set TonerProSeite_extrapol = CASE WHEN d_VolumenToner>0 THEN (100-BefChValue_extrapol)/d_VolumenToner END where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;&#xD;&#xA;update dbo.STAT_TonerZaehlerChange set TonerNetto_DeckDiff = (1-SupplyCapacity*TonerProSeite_extrapol/100)*(100-BefChValue_extrapol) from STAT_TonerZaehlerChange where TonerProSeite_extrapol>0 and Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;&#xD;&#xA;update dbo.STAT_TonerZaehlerChange set LossByTonerChange_EUR =  &#x9;(BefChValue_extrapol)/100*SupplyPrice where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;&#xD;&#xA;update dbo.STAT_TonerZaehlerChange set NettoByTDeckDiff_EUR =  &#x9;(TonerNetto_DeckDiff)/100*SupplyPrice where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;&#xD;&#xA;update dbo.STAT_TonerZaehlerChange set TonerDeckung =  &#x9;SupplyCapacity/100*TonerProSeite_extrapol* (case when TonerDeckung is null then 5 else TonerDeckung end) where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_TonerZaehlerChange set  SCap_SPrice_DVolFarbe_DVGes_Flag =  CASE WHEN SupplyCapacity&lt;2 THEN SCap_SPrice_DVolFarbe_DVGes_Flag+1&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE CASE WHEN SupplyPrice=0 THEN SCap_SPrice_DVolFarbe_DVGes_Flag+2&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE CASE WHEN DeadVol_Percent_Farbe is null THEN SCap_SPrice_DVolFarbe_DVGes_Flag+4&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE CASE WHEN DeadVol_Percent_Gesamt is null THEN SCap_SPrice_DVolFarbe_DVGes_Flag+8&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 0&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END END END END where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4&#xD;&#xA;&#xD;&#xA;Update dbo.STAT_TonerZaehlerChange set ExtrapolQuality = CASE&#x9;WHEN  NoTonerChange_Flag=1 THEN 'No Toner Change' &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE CASE&#x9;WHEN DeadVol_Percent_Gesamt is null THEN 'No Extrapolation'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN DeadVol_Percent_Gesamt &lt;=25 THEN 'Good'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN DeadVol_Percent_Gesamt>25 and DeadVol_Percent_Gesamt&lt;=50 THEN 'Satisfying'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN DeadVol_Percent_Gesamt>25 and DeadVol_Percent_Gesamt&lt;=90 THEN 'Bad'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN DeadVol_Percent_Gesamt >90 THEN 'Not Acceptable'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END END where Ebene1=:1 and Ebene2=:2 and Ebene3=:3 and Ebene4=:4"/>
