        <SQL_FORM DB="SQL" RESTAB="SQL">
          <SQL_STMT_U val="insert into stat_artikel_idnr_bestand (Ebene1,Ebene2,Ebene3,Ebene4,artikelnr, idnr, statistikgruppe, lagernr, bestand,gebraucht,ek)&#xA;select a.Ebene1,a.Ebene2,a.Ebene3,a.Ebene4,a.artikelpart_nr, 0 as IDNR, ag.statistiknummer, nl.lagernr, nl.bestand, 0 as gebraucht,0 as ek&#xA;from artikel a&#xA;join nebenlager nl on (nl.ArtikelNr=a.Artikelpart_nr and nl.Ebene1=:5 and nl.Ebene2=:6 and nl.Ebene3=:7 and nl.Ebene4=:8)&#xA;left outer join artikelgruppe ag on (ag.Artikelgruppe=a.artikelgruppe and ag.Ebene1=:5 and ag.Ebene2=:6 and ag.Ebene3=:7 and ag.Ebene4=:8)&#xA;join kunden kd on (kd.Ebene1=nl.Ebene1 and kd.Ebene2=nl.Ebene2 and kd.Ebene3=nl.Ebene3 and kd.Ebene4=nl.Ebene4 and kd.KDNR=nl.LagerNr and kd.Kontoart='W')&#xA;where a.zuordnung &lt;>'M' and nl.LagerNr not in (0,3,4,5,6,7,8,9,10,11,17,18,19,20,29,39,49,88,97,98,99) and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xA; &#xA;union&#xA;&#xA;select a.Ebene1,a.Ebene2,a.Ebene3,a.Ebene4,a.artikelpart_nr, 0 as IDNR, ag.statistiknummer, nl.lagernr, nl.bestand, 0 as gebraucht,0 as ek&#xA;from artikel a&#xA;join nebenlager nl on (nl.ArtikelNr=a.Artikelpart_nr and nl.Ebene1=:5 and nl.Ebene2=:6 and nl.Ebene3=:7 and nl.Ebene4=:8)&#xA;left outer join artikelgruppe ag on (ag.Artikelgruppe=a.artikelgruppe and ag.Ebene1=:5 and ag.Ebene2=:6 and ag.Ebene3=:7 and ag.Ebene4=:8)&#xA;where a.zuordnung &lt;>'M' and nl.LagerNr=0 and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xA;&#xA;union&#xA;&#xA;select a.Ebene1,a.Ebene2,a.Ebene3,a.Ebene4,a.artikelpart_nr, g.idnr, statistiknummer, g.betreiber, 1, g.gebrauchtgeraet, isnull(g.einkaufspreis,0)&#xA;from artikel a&#xA;left outer join artikelgruppe ag on (ag.Artikelgruppe=a.artikelgruppe and ag.Ebene1=:5 and ag.Ebene2=:6 and ag.Ebene3=:7 and ag.Ebene4=:8)&#xA;join geraete g on (g.artikelnummer=a.artikelpart_nr and g.Ebene1=:5 and g.Ebene2=:6 and g.Ebene3=:7 and g.Ebene4=:8)&#xA;join kunden kd on (kd.Ebene1=g.Ebene1 and kd.Ebene2=g.Ebene2 and kd.Ebene3=g.Ebene3 and kd.Ebene4=g.Ebene4 and kd.KDNR=g.Betreiber and kd.Kontoart='W')&#xA;where a.zuordnung ='M' and (g.nebenlagernr=0 and g.Betreiber=1 or nebenlagernr>0) and g.nebenlagernr not in (3,4,5,6,7,8,9,10,11,17,18,19,20,29,39,49,88,97,98,99)&#xA;and a.Ebene1=:1 and a.Ebene2=:2 and a.Ebene3=:3 and a.Ebene4=:4&#xA;"/>
